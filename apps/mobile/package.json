{
  "name": "video-conference-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export:web": "expo export --platform web --clear && node scripts/inject-preload.js",
    "build:web:vite": "vite build && node -e \"const fs=require('fs'); const p='dist-web/index-vite.html'; if(fs.existsSync(p)){ fs.renameSync(p,'dist-web/index.html'); }\"",
    "preview:web:vite": "vite build --mode development && node -e \"const fs=require('fs'); const p='dist-web/index-vite.html'; if(fs.existsSync(p)){ fs.renameSync(p,'dist-web/index.html'); }\" && vite preview --outDir dist-web"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "expo": "~52.0.0",
    "expo-asset": "^12.0.12",
    "expo-constants": "~17.0.0",
    "expo-font": "^14.0.11",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-web": "^0.19.13",
    "react-native-webrtc": "^118.0.7",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@vitejs/plugin-react": "^5.1.4",
    "expo-atlas": "^0.4.3",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "vite": "^7.3.1"
  },
  "private": true
}
